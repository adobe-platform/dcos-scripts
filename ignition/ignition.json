      },
      {
        "name": "coreos-curl.service",
        "enable": true,
        "contents": "[Unit]\nDescription=Address incompatible libcurl on CoreOS\nRequires=dcos-download.service\nAfter=dcos-download.service\nBefore=dcos-setup.service\n[Service]\nType=oneshot\nStandardOutput=journal+console\nStandardError=journal+console\nExecStart=/bin/bash -c 'sed -i s,/usr/bin/curl,/opt/mesosphere/bin/curl, $(find /opt/mesosphere -type f -print0 | xargs -0 fgrep -l /usr/bin/curl) ||:'\n[Install]\nWantedBy=multi-user.target\n"
      },
      {
        "name": "promethos-exporter-setup.service",
        "contents": "[Unit]\nDescription=Download Prometheus exporters\nConditionFileIsExecutable=|!/opt/blackbox_exporter/blackbox_exporter\nConditionFileIsExecutable=|!/opt/cadvisor/cadvisor\nConditionFileIsExecutable=|!/opt/node_exporter/node_exporter\nConditionFileIsExecutable=|!/opt/process_exporter/process_exporter\nAfter=network-online.target\n[Service]\nType=oneshot\nEnvironmentFile=/etc/ethos/promethos.env\nExecStart=/opt/ethos/bin/download-promethos-exporters\n[Install]\nWantedBy=multi-user.target\n"
      },
      {
        "name": "blackbox-exporter.service",
        "enable": true,
        "contents": "[Unit]\nDescription=Prometheus Blackbox Exporter Service\nConditionFileIsExecutable=/opt/blackbox_exporter/blackbox_exporter\nAfter=promethos-exporter-setup.service\n[Service]\nType=simple\nRestart=on-failure\nExecStart=/opt/blackbox_exporter/blackbox_exporter -config.file /opt/blackbox_exporter/blackbox.yml\n[Install]\nWantedBy=multi-user.target\n"
